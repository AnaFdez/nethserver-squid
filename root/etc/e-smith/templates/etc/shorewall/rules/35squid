{
    $status = $squid{'status'} || 'disabled';
    $mode = $squid{'Mode'} || 'manual';
    if ($status eq 'enabled') {
        if ($mode eq 'transparent') {
            $OUT.="# Squid: accept connection from firewall to external sites\n";
            $OUT.="ACCEPT\t\$FW\tnet\ttcp\t80";
        }
    }
}
